{
    "name":"my_packe/my_insta",
    "description": "this app offers stacking posts together",
    "license": "proprietary",
    "require": {
        "php":">=8.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "monolog/monolog": "^3.7",
        "phpunit/phpunit": "^9.5"
    }
    ,"autoload":{
        "psr-4": {
            "Insta\\":"php/Insta"
            
        },
        "classmap": [
            "index.php"
        ],
        "files": [
            "php/",
            "index.php"
        ]
    },
    "config": {
        "lock": true,
        "sort-packages": true,
        "platform-check": false
    }
}
