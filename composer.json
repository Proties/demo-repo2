{
    "require": {
        "php":">=8.1",
        "monolog/monolog": "^3.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit":"^9.5"
    }
    ,"autoload":{
        "classmap": [
            "php/",
            "index.php"
        ],
        "files": [
            "index.php"
        ]
    },
    "config": {
        "composer-version":"2.2.24",
        "lock": true,
        "sort-packages": true,
        "platform-check": false
    }
}
