{
    "name":"Insta",
    "require": {
        "php":">=8.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "monolog/monolog": "^3.7",
        "phpunit/phpunit": "^9.5"
    }
    ,"autoload":{
        "psr-4": {
            "Insta\\":"php/Insta"
            
        },
        "classmap": [
            "index.php"
        ],
        "files": [
            "php/",
            "index.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "":"tests/"
        }
    },
    "scripts"{
        
    },
    "config": {
        "lock": false,
        "sort-packages": true,
        "platform-check": false
    }
}
