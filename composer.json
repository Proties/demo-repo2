{
    "require": {
        "php":">=8.1",
        "monolog/monolog": "^3.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit":"^9.5"
    }
    ,"autoload":{
        "psr-4": {
            "":"php/",
            "Monolog\\":"vendor/mononlog/mononlog/src",
            "Dotenv\\":"vendor/vlucas/phpdotenv/src",
            "Users\\":"php/",
            "Posts\\":"php/",
            "Categories\\":"php/"
        },
        "classmap": [
            "php/",
            "index.php"
        ],
        "files": [
            "index.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "":"tests/"
        }
    },
    "config": {
        "lock": true,
        "sort-packages": true,
        "platform-check": false
    }
}
